<%= render "books/list",user: @user, book: @book %>

<% @books.each do |book| %>
<div>
 <%= link_to user_path(book.user) do %>
  <%= attachment_image_tag book.user, :profile_image,:fill, 60, 60, fallback: "no_image.jpg" %>
 <% end %>
</div>
<p><%= link_to book.title, book_path(book) %></p>
<p><%= book.body %></p>
<% end %>