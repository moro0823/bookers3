<%= render "books/list",user: @user, book: @book %>

<h1>Books</h1>
<p>
<% @books.each do |book| %>
<%= link_to user_path(@user.id) do %>
 <%= attachment_image_tag @user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg" %>
<% end %>
<%= link_to book.title,book_path(book.id) %>
<%= book.body %>
</p>
<% end %>

