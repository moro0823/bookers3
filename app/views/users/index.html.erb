<%= render "books/list",user: @user, book: @book %>

<% @users.each do |user| %>
<div>
 <%= link_to user_path(user.id) do %>
  <%= attachment_image_tag user, :profile_image,:fill, 60, 60, fallback: "no_image.jpg" %>
 <% end %>
</div>
<p><%= user.name %></p>
<p><%= link_to"Show", user_path(user.id) %></p>
<% end %>